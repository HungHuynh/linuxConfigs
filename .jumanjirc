searchengine google http://www.google.com/search?q=%s
searchengine wikipedia https://secure.wikimedia.org/wikipedia/en/w/index.php?search=%s

# browser settings
set homepage http://calendar.google.com
set auto_save 60

# look
set font droid sans medium 9

# key bindings
map <C-q> quit

# follow hint
script ~/.config/jumanji/scripts/hints.js

# custom css
#set stylesheet ~/.config/jumanji/style.css

# proxy
# Search engines
searchengine g http://www.google.com/search?q=%s
# Downloads
set download_dir /ntfs-data/inbox/
set download_command urxvt -e sh -c "wget --load-cookies ~/.config/jumanji/cookies '%s' -O %s";
