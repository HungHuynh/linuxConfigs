! Urxvt
URxvt.buffered: true
URxvt.background: #121212
URxvt.foreground: #999999
URxvt.cursorColor: #5E468C
URxvt.cursorBlink: true
URxvt.geometry: 80x22
URxvt.transparent: 0
URxvt*urgentOnBell: true
URxvt.letterSpace: .5

! -- Xft settings -- !
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hinstyle:   hintsfull

! borderless and no scrollbar
URxvt*scrollBar_right: false
URxvt*scrollBar: false
URxvt*borderLess: false

! teh transparency stuff
URxvt*inheritPixmap: false
URxvt*tintColor: #080808
URxvt*shading: 0

! -- Fonts -- !
!URxvt.font:xft:Droid Sans Mono Slashed:medium:size=8.6
!URxvt.boldFont:xft:Droid Sans Mono Slashed:bold:size=8.6
URxvt.font:xft:monaco:medium:size=8
URxvt.boldFont:xft:monaco:bold:size=8

!URxvt.font:xft:envy code r:medium:size=9
!URxvt.boldFont:xft:envy code r:medium:size=9
!URxvt.font:xft:inconsolata:bold:pixelsize=12
!URxvt.font:     -*-dina-medium-r-normal-*-*-*-*-*-*-*-*-*
!URxvt.boldFont: -*-dina-medium-r-normal-*-*-*-*-*-*-*-*-*
!URxvt.font:     -*-terminus-medium-r-*-*-16-*-*-*-*-*-*-*
!URxvt.boldFont: -*-terminus-medium-r-*-*-16*-*-*-*-*

! -- Colours -- !
#include "/home/nxqd/.colours/colorful"
!#include "/home/nxqd/.colours/low"
!#include "/home/nxqd/.colours/old"
!#include "/home/nxqd/.colours/soft"
!#include "/home/nxqd/.colours/washed"
!#include "/home/nxqd/.colours/bright"
!#include "/home/nxqd/.colours/lich"
!#include "/home/nxqd/.colours/rab"
!#include "/home/nxqd/.colours/reasons"
!#include "/home/nxqd/.colours/zenburn"

! -- Clickable urls -- !
URxvt.perl-ext-common: default,matcher
URxvt.urlLauncher: /home/nxqd/Scripts/vimprobtab.sh
URxvt.matcher.button: 1
URxvt.matcher.rend.0: fg16 RS_Uline

