
# browser settings
set homepage http://calendar.google.com

# auto save session / bookmarks
set auto_save 60

# look
set font droid sans mono medium 8

# key bindings
map <C-q> quit

# session
set default_session_name start
set session_restored true

# follow hint
script ~/.config/jumanji/scripts/hints.js

# custom css
set stylesheet ~/.config/jumanji/nXqd.css


# Enable plugins
plugin application/x-shockwave-flash

# proxy
# Search engines
searchengine g http://www.google.com/search?q=%s
searchengine imdb http://www.imdb.com/find?s=all&q=%s
searchengine timer http://e.ggtimer.com/%s

# Downloads
set download_dir /ntfs-data/inbox/
set download_command urxvt -e wget --load-cookies ~/.config/jumanji/cookies '%s' -O '%s';
